{
  "name": "@affanmomin/agent-workspace",
  "version": "1.1.0",
  "type": "module",
  "description": "Customizable AI agent framework distribution for different project types",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-kit": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/init.ts",
    "test": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "release": "standard-version",
    "commit": "cz",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "cli",
    "scaffold",
    "development",
    "automation"
  ],
  "author": "Affan Momin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/affanmomin/agent-workspace.git"
  },
  "homepage": "https://github.com/affanmomin/agent-workspace#readme",
  "bugs": {
    "url": "https://github.com/affanmomin/agent-workspace/issues"
  },
  "files": [
    "dist",
    "bin",
    ".agent",
    "README.md"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
